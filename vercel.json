{
  "version": 2,
  "builds": [
    { "src": "src/app.ts", "use": "@vercel/node@5.3.22" },
    { "src": "src/public/**/*", "use": "@vercel/static" },
    { "src": "src/views/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/output.css", "dest": "/public/output.css" },
    { "src": "/(.*)", "dest": "/src/app.ts" }
  ]
}
